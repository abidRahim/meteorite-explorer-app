{
	"extends": "airbnb",
	"rules": {
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [
					".js",
					".jsx"
				]
			}
		],
		"react/prefer-stateless-function": [
			"<enabled>",
			{
				"ignorePureComponents": "<ignorePureComponents>"
			}
		],
		"react/prop-types": [
			"<enabled>",
			{
				"ignore": "<ignore>",
				"customValidators": "<customValidator>"
			}
		]
	}
}